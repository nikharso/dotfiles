{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "dank mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.lineHeight": 1.5,
    "editor.linkedEditing": true,
    "editor.minimap.autohide": true,
    "editor.minimap.showSlider": "always",
    "editor.mouseWheelZoom": true,
    "editor.occurrencesHighlight": "off",
    "editor.renderWhitespace": "none",
    "editor.stickyScroll.enabled": false,
    "editor.tabSize": 4,
    "explorer.compactFolders": false,
    "explorer.confirmPasteNative": false,
    "explorer.decorations.colors": false,
    "scm.diffDecorations": "gutter",
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "prettier.printWidth": 128,
    "window.commandCenter": false,
    "window.title": "${dirty}${activeEditorShort}${separator}${folderName}",
    "window.zoomLevel": 0.7,
    "workbench.colorCustomizations": {
        "[Rosé Pine (no italics)]": {
            "editorCursor.foreground": "#eacfce",
            "editorCursor.background": "#21022a"
        }
    },
    "workbench.colorTheme": "Rosé Pine (no italics)",
    "workbench.editor.decorations.badges": false,
    "workbench.iconTheme": "symbols",
    "workbench.productIconTheme": "Tabler",
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-p>": false,
        "<C-y>": false
    },
    "vim.incsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.textwidth": 200,
    "vim.useSystemClipboard": true,
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "p"
            ],
            "after": [
                "p",
                "g",
                "v",
                "y"
            ]
        }
    ],
    "vim.visualstar": true,
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "catppuccin-noctis-icons.hidesExplorerArrows": false,
    "editor.minimap.renderCharacters": false,
    "transparent-win.transparency": 0.3,
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "terminal.integrated.fontSize": 15,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.activityBar.orientation": "vertical"
}
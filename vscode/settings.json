{
  "custom-ui-style.font.sansSerif": "Hack Nerd Font",
  "window.zoomLevel": -0.5,
  "workbench.colorTheme": "Kanso Ink",
  "editor.smoothScrolling": true,
  "workbench.panel.opensMaximized": "always",
  "window.confirmBeforeClose": "always",
  "editor.fontFamily": "Hack Nerd Font",
  "editor.lineHeight": 1.6,
  "terminal.integrated.fontSize": 12,
  "editor.fontSize": 13,
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.letterSpacing": 1,
  "scm.diffDecorations": "gutter",
  "editor.lineNumbers": "relative",
  "editor.wordWrap": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // "workbench.activityBar.orientation": "vertical",
  "editor.occurrencesHighlight": "off",
  "editor.renderWhitespace": "none",
  "editor.stickyScroll.enabled": false,
  "editor.cursorSurroundingLines": 10,
  "explorer.compactFolders": false,
  "explorer.confirmPasteNative": false,
  "explorer.decorations.colors": false,

  "vim.easymotion": true,
  "vim.leader": "<space>",
  "vim.foldfix": true,
  "vim.useCtrlKeys": true,
  "vim.textwidth": 200,
  "vim.showcmd": false,
  "vim.useSystemClipboard": true,
  "vim.overrideCopy": false,
  "vim.changeWordIncludesWhitespace": true,
  "vim.handleKeys": {
    "<C-a>": false, // fslfjks ljk
    "<C-f>": false,
    "<C-p>": false,
    "<C-y>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    },
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
    },
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
    },
    {
      "before": ["n"],
      "after": ["n", "z", "z"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    }
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "git.autofetch": true,
  "css.lint.unknownAtRules": "ignore",
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "editor.cursorBlinking": "blink",
  "catppuccin-noctis-icons.hidesExplorerArrows": false,
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[mdx]": {
    "editor.defaultFormatter": "unifiedjs.vscode-mdx"
  },
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "workbench.experimental.share.enabled": true,
  "cursor.composer.usageSummaryDisplay": "always",
  "color-highlight.markRuler": false,
  "cSpell.useCustomDecorations": true,
  "cSpell.showInRuler": false,
  "workbench.navigationControl.enabled": false,
  "workbench.iconTheme": "catppuccin noctis icons",
  "workbench.activityBar.location": "top",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "chat.commandCenter.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
